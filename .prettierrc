{
    "proseWrap": "never",
    "plugins": [
        "prettier-plugin-go-template",
        "prettier-plugin-tailwind-css"
    ],
    "overrides": [
        {
            "files": [
                "*.html"
            ],
            "options": {
                "parser": "go-template"
            }
        }
    ],
    "tailwindStylesheet": "./assets/css/styles.css"
}
